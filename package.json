{
	"name": "mine-chief",
	"version": "1.0.0",
	"description": "mine-chief",
	"main": "''",
	"scripts": {
		"clean": "rimraf public",
		"prepare": "npm run clean && node script.js -c",
		"build": "npm run prepare && webpack --config webpack.production.config.js --progress --profile --colors",
		"start": "npm run prepare && webpack-dev-server --progress --profile --colors",
		"lint": "eslint --ext js --ext jsx src || exit 0",
		"dev": " webpack-dashboard -- webpack-dev-server",
		"test": "export NODE_ENV=test && jest",
		"build:router": "node build/router.js",
		"upload": "node script.js -u",
		"publish": "npm run build && npm run upload",
		"publish:dev": "node script.js -d && npm run publish",
		"publish:prod": "node script.js -p && npm run publish",
		"publish:all": "npm run publish:dev && npm run publish:prod"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kecoyo/mine-chief"
	},
	"author": "beefly",
	"license": "MIT",
	"homepage": "https://github.com/kecoyo/mine-chief#readme",
	"dependencies": {
		"classnames": "^2.2.5",
		"jeselvmo": "^1.0.7",
		"jquery": "^3.2.1",
		"lodash": "^4.17.4",
		"mobx": "^3.3.0",
		"mobx-logger": "^0.6.0",
		"mobx-react": "^4.3.2",
		"react": "^15.6.2",
		"react-amap": "^1.2.0",
		"react-dom": "^15.6.2",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2"
	},
	"devDependencies": {
		"ali-oss": "^4.11.3",
		"babel-core": "^6.23.1",
		"babel-eslint": "^8.2.1",
		"babel-jest": "^21.2.0",
		"babel-loader": "^7.1",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-plugin-transform-class-properties": "^6.22.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-runtime": "^6.22.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.23.0",
		"babel-preset-stage-0": "^6.24.1",
		"babel-runtime": "^6.22.0",
		"bundle-loader": "^0.5.5",
		"copy-webpack-plugin": "^4.2.3",
		"css-loader": "^0.28.7",
		"enzyme": "^3.2.0",
		"eslint": "^4.3.0",
		"eslint-plugin-jest": "^21.3.2",
		"eslint-plugin-react": "^7.1.0",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^1.1.5",
		"html-webpack-plugin": "^2.26.0",
		"jest": "^21.2.1",
		"loglevel": "^1.6.0",
		"node-sass": "^4.3.0",
		"postcss-loader": "^2.0.8",
		"react-hot-loader": "^3.1.3",
		"react-test-renderer": "^16.2.0",
		"rimraf": "^2.6.2",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.0",
		"uglifyjs-webpack-plugin": "^1.1.6",
		"url-loader": "^0.6.2",
		"webpack": "^3.8.1",
		"webpack-cleanup-plugin": "^0.5.1",
		"webpack-dashboard": "^1.0.2",
		"webpack-dev-server": "^2.9.7"
	},
	"jest": {
		"verbose": true,
		"bail": true,
		"collectCoverage": true,
		"coverageDirectory": "coverage",
		"moduleNameMapper": {
			"\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
			"\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js",
			"app": "<rootDir>/src/app.jsx"
		},
		"moduleFileExtensions": [
			"js",
			"jsx"
		],
		"moduleDirectories": [
			"node_modules"
		]
	}
}
